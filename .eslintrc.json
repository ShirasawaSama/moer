{
  "extends": "standard",
  "plugins": [
    "standard",
    "promise",
    "import",
    "node"
  ],
  "parser": "babel-eslint",
  "rules": {
    "no-new": "off",
    "no-var": "error",
    "no-labels": "off",
    "no-lone-blocks": ["off"],
    "no-unused-expressions": "off"
  },
  "env": {
    "jest": true
  }
}